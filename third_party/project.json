{
  "name": "third_party",
  "projectType": "library",
  "sourceRoot": "third_party",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vendir sync",
        "cwd": "third_party"
      },
      "cache": true,
      "inputs": ["{projectRoot}/vendir.yml", "{projectRoot}/vendir.lock.yml"],
      "outputs": ["{projectRoot}/charts"]
    }
  }
}
