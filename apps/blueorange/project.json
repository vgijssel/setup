{
  "name": "blueorange",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/blueorange/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/blueorange"],
      "options": {
        "command": "vite build",
        "cwd": "apps/blueorange"
      },
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**/*",
        "{projectRoot}/public/**/*",
        "{projectRoot}/index.html",
        "{projectRoot}/vite.config.*",
        "{projectRoot}/package.json",
        "{projectRoot}/tsconfig*.json"
      ]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "apps/blueorange"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "apps/blueorange"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "apps/blueorange"
      },
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**/*",
        "{projectRoot}/tests/**/*",
        "{projectRoot}/vite.config.*",
        "{projectRoot}/vitest.config.*",
        "{projectRoot}/package.json",
        "{projectRoot}/tsconfig*.json"
      ]
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "playwright test",
        "cwd": "apps/blueorange"
      }
    }
  },
  "tags": []
}
