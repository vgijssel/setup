name: bunq2ynab
version: '3.8'
services:
  bunq2ynab:
    container_name: bunq2ynab
    image: ghcr.io/vgijssel/setup/bunq2ynab:{{ bunq2ynab_image_tag }}
    restart: always
    environment:
      - OP_SERVICE_ACCOUNT_TOKEN={{ op_service_account_token }}
      - SETUP_ENV={{ setup_env }}
    deploy:
      resources:
        limits:
          cpus: '0.5' 
          memory: '256M' 
    labels:
      containerName: "bunq2ynab"
      env: "{{ setup_env }}"