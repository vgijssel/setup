{
  "Name": "{{ network_interface }}",
  "Proxy": true,
  "Enabled": true,
  "Subnet": "{{ network_cidr }}",
  "ActiveStart": "{{ network_dhcp_start }}",
  "ActiveEnd": "{{ network_dhcp_end }}",
  "Options": [
    {
      "Code": 1,
      "Value": "{{ network_netmask }}"
    },
    {
      "Code": 28,
      "Value": "{{ network_broadcast }}"
    },
    {
      "Code": 67,
      "Value": "tftp://{{ provisioner_ip }}/lpxelinux.0"
    }
  ],
  "Strategy": "MAC",
}
