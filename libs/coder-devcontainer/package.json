{
  "name": "coder-devcontainer",
  "version": "0.8.0",
  "dependencies": {
    "devcontainer": "0.6.12"
  },
  "nx": {
    "projectType": "library",
    "sourceRoot": "libs/coder-devcontainer",
    "targets": {
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "terraform init",
            "terraform validate"
          ],
          "parallel": false,
          "cwd": "libs/coder-devcontainer"
        },
        "cache": true,
        "metadata": {
          "description": "Test the Coder template configuration"
        }
      }
    },
    "tags": [
      "coder",
      "devcontainer",
      "terraform",
      "development-environment"
    ]
  },
  "private": true
}
