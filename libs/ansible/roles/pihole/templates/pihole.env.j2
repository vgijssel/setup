# Pi-hole Docker Compose Environment Configuration
# Managed by Ansible - DO NOT EDIT MANUALLY

# Docker image (pinned version)
PIHOLE_DOCKER_IMAGE={{ pihole_docker_image }}

# VLAN 50 Pi-hole (Kubernetes network)
PIHOLE_VLAN50_IP={{ pihole_vlan50_ip }}
PIHOLE_VLAN50_GATEWAY={{ pihole_vlan50_gateway }}
PIHOLE_VLAN50_SUBNET={{ pihole_vlan50_subnet }}
PIHOLE_VLAN50_PARENT_INTERFACE={{ pihole_vlan50_parent_interface }}
PIHOLE_VLAN50_CONTAINER_NAME={{ pihole_vlan50_container_name }}
PIHOLE_VLAN50_DATA_DIR={{ pihole_vlan50_data_dir }}

# Admin LAN Pi-hole
PIHOLE_ADMIN_IP={{ pihole_admin_ip }}
PIHOLE_ADMIN_GATEWAY={{ pihole_admin_gateway }}
PIHOLE_ADMIN_SUBNET={{ pihole_admin_subnet }}
PIHOLE_ADMIN_PARENT_INTERFACE={{ pihole_admin_parent_interface }}
PIHOLE_ADMIN_CONTAINER_NAME={{ pihole_admin_container_name }}
PIHOLE_ADMIN_DATA_DIR={{ pihole_admin_data_dir }}

# Shared configuration
PIHOLE_UPSTREAM_DNS={{ pihole_upstream_dns }}
PIHOLE_TIMEZONE={{ pihole_timezone }}
PIHOLE_WEB_PASSWORD={{ pihole_web_password }}
