{
  "name": "internal-dns",
  "version": "1.1.0",
  "type": "commonjs",
  "nx": {
    "projectType": "library",
    "sourceRoot": "libs/internal-dns",
    "targets": {
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "command": "kuttl test",
          "cwd": "libs/internal-dns",
          "start-kind": true,
          "skip-cluster-delete": false,
          "configurations": {
            "dev": {
              "start-kind": false,
              "skip-cluster-delete": true
            }
          }
        },
        "cache": true,
        "inputs": [
          "{projectRoot}/**/*",
          "!{projectRoot}/kind-logs-*/**/*",
          "!{projectRoot}/kubeconfig",
          {
            "env": "NX_TASK_TARGET_CONFIGURATION"
          }
        ],
        "outputs": []
      }
    },
    "tags": [
      "type:library",
      "scope:infrastructure",
      "platform:kubernetes"
    ]
  },
  "private": true
}
