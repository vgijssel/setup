{
  "name": "internal-dns",
  "version": "2.0.0",
  "nx": {
    "projectType": "library",
    "sourceRoot": "libs/internal-dns",
    "targets": {
      "test": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "dev-cluster create internal-dns-test",
            "chainsaw test --test-dir tests --kube-context kind-internal-dns-test",
