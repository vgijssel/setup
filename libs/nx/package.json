{
  "name": "@setup/nx",
  "version": "1.0.0",
  "type": "commonjs",
  "nx": {
    "sourceRoot": ".",
    "projectType": "library",
    "targets": {
      "release_plan_test": {
        "executor": "nx:run-commands",
        "cache": false,
        "options": {
          "command": "nx release plan:check",
          "cwd": "."
        }
      },
      "release_test": {
        "executor": "nx:run-commands",
        "cache": true,
        "inputs": [
          "{workspaceRoot}/**/package.json",
          "{workspaceRoot}/**/package-lock.json",
          "{workspaceRoot}/nx.json",
          "{workspaceRoot}/.nx/version-plans/*.md"
        ],
        "options": {
          "command": "nx release --dry-run",
          "cwd": "."
        }
      },
      "package_test": {
        "executor": "nx:run-commands",
        "cache": true,
        "inputs": [
          "{workspaceRoot}/**/package.json",
          "{workspaceRoot}/**/package-lock.json"
        ],
        "options": {
          "command": "npm install --package-lock-only --dry-run",
          "cwd": "."
        }
      },
      "test": {
        "executor": "nx:noop",
        "dependsOn": [
          "release_plan_test",
          "release_test",
          "package_test"
        ]
      }
    }
  }
}
