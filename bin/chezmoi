#!/bin/bash

# Not using SETUP_DIR here, as this might not be available as this bin/ can be used outside the repo root
# by symlinking it to ~/.local/setup/bin
LOCAL_SETUP_DIR="$(dirname "$(realpath "$0")")/.."
exec "${LOCAL_SETUP_DIR}"/bin/internal/bin/chezmoi --config "${LOCAL_SETUP_DIR}"/libs/devenv/chezmoi.yaml -S "${LOCAL_SETUP_DIR}" "$@"