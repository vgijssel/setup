{
  "name": "devcontainer",
  "sourceRoot": ".devcontainer",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "devcontainer build --workspace-folder ."
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": ".devcontainer/test.sh",
        "cwd": "."
      }
    }
  }
}
